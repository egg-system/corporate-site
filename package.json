{
  "name": "eggsystem",
  "version": "1.0.0",
  "description": "eggsystem",
  "author": "eggsystem",
  "private": true,
  "scripts": {
    "test": "jest",
    "dev": "nuxt",
    "dev:port": "PORT=50000 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/device": "^1.2.7",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/google-adsense": "^1.1.3",
    "@nuxtjs/sitemap": "^2.4.0",
    "axios": "^0.19.2",
    "bulma": "^0.7.5",
    "dayjs": "^1.8.35",
    "file-loader": "^5.0.2",
    "jquery": "^3.5.1",
    "nuxt": "^2.14.5",
    "url-loader": "^3.0.0",
    "vue": "^2.6.12",
    "vue-scrollto": "^2.18.2",
    "vue-server-renderer": "^2.6.12",
    "vue-slick": "^1.1.15",
    "vuex-persistedstate": "^3.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
